{
  "name": "@xnomi/cache",
  "version": "1.1.0",
  "description": "The cache package for localStorage, sessionStorage and Cookie",
  "main": "./dist/cache.cjs",
  "module": "./dist/cache.mjs",
  "unpkg": "./dist/cache.iife.min.js",
  "jsdelivr": "./dist/cache.iife.min.js",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cache.cjs",
      "import": "./dist/cache.mjs"
    }
  },
  "scripts": {
    "build": "rimraf ./dist && microbundle --name xnomiCache",
    "dev": "microbundle watch"
  },
  "keywords": [
    "cache",
    "storage",
    "cookie"
  ],
  "author": "zhicaizhu123 <zhicaizhu123@126.com>",
  "license": "MIT",
  "dependencies": {
    "@xnomi/transfer": "workspace:*",
    "js-cookie": "^3.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhicaizhu123/xnomi.git"
  },
  "bugs": {
    "url": "https://github.com/zhicaizhu123/xnomi/issues"
  }
}
